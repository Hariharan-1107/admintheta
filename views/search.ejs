
<style>
    /* Set the font family and size for the page */
    body {
      font-family: Arial, sans-serif;
      font-size: 16px;
    }
  
    /* Set the style for the container div */
    .container {
      max-width: 1200px;
      margin: 0 auto;
      padding: 20px;
    }
    
    /* Set the style for the user information table */
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 20px;
    }
  
    /* Set the style for table header cells */
    th {
      background-color: #2c3e50;
      color: white;
      font-weight: bold;
      padding: 10px;
      text-align: left;
    }
  
    /* Set the style for table cells */
    td {
      border: 1px solid #ddd;
      padding: 10px;
    }
  
    /* Set the style for table row hover */
    tr:hover {
      background-color: #f5f5f5;
    }
    
    /* Set the style for the user information headings */
    h1, h2 {
      text-align: center;
      margin-top: 50px;
      margin-bottom: 20px;
    }
  </style>

  <%if(error){%>
        <h1 class="container">Not Found</h1>
  <%}else{%>
  <div class="container">
    <h1>Found</h1>
    <table>
      <thead>
        <tr>
          <th>User ID</th>
          <th>Name</th>
          <th>College</th>
          <th>Email</th>
          <th>Phone</th>
          <th>Events Registered</th>
        </tr>
      </thead>
      <tbody>
                    <tr>
                    <div class="rectangle">
                        <td><%=obj.id%></td>
                        <td><%=obj.fname%> <%=obj.lname%></td>
                        <%if(obj.Sastraite){%>
                            <td>Sastra</td>
                            <%}else{%>
                              <td><%=obj.collegeName%></td>
                        <%}%>
                        <td><%=obj.username%></td>
                        <td><%=obj.ph%></td>
                        <td><%=obj.events.eventArray%></td>
                      </div>
                    </tr>

       
        <!-- Add more rows for each user information -->
      </tbody>
    </table>
  </div>

  <%}%>
  